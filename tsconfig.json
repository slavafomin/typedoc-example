{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@tsconfig/node-lts-strictest-esm/tsconfig.json",
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "ES2020"
    ],
    "sourceMap": true,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "outDir": "dist",
    "declaration": true,
    "declarationMap": true,
    "declarationDir": "temp/types",
    "isolatedModules": true
  },
  "files": [
    "index.ts"
  ]
}
